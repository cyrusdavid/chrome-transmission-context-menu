{
  "name": "Transmission Context Menu",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "icons": {
    "128": "icon-128.png",
    "48": "icon-48.png",
    "16": "icon-16.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "storage",
    "contextMenus",
    "notifications",
    "<all_urls>"
  ]
}
